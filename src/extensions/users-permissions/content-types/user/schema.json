{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "profile_photo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "landscape_photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "university": {
      "type": "string",
      "required": true,
      "maxLength": 50,
      "minLength": 1
    },
    "description": {
      "type": "text",
      "minLength": 1,
      "maxLength": 300,
      "required": true
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "mappedBy": "students"
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50,
      "unique": false,
      "required": true
    },
    "role_str": {
      "type": "string",
      "required": true
    },
    "calendar_events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::calendar-event.calendar-event",
      "inversedBy": "users"
    },
    "user_response_questionnaires": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-response-questionnaire.user-response-questionnaire",
      "mappedBy": "user"
    },
    "qualifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::qualification.qualification",
      "mappedBy": "user"
    },
    "user_objectives": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-objective.user-objective",
      "mappedBy": "user"
    },
    "log": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::log.log",
      "inversedBy": "user"
    },
    "activities": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::activity.activity",
      "inversedBy": "evaluators"
    },
    "notifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::notification.notification",
      "mappedBy": "users"
    },
    "subsections_completed": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subsection.subsection",
      "mappedBy": "users_who_completed"
    },
    "peer_review_answer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::peer-review-answer.peer-review-answer",
      "mappedBy": "user"
    }
  }
}
