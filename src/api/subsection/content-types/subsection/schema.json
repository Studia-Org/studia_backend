{
  "kind": "collectionType",
  "collectionName": "subsections",
  "info": {
    "singularName": "subsection",
    "pluralName": "subsections",
    "displayName": "Subsection",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "fase": {
      "type": "string",
      "required": true
    },
    "finished": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "start_date": {
      "type": "datetime"
    },
    "end_date": {
      "type": "datetime"
    },
    "activity": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::activity.activity"
    },
    "description": {
      "type": "string",
      "maxLength": 150,
      "minLength": 1,
      "required": true
    },
    "landscape_photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "questionnaire": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::questionnaire.questionnaire",
      "inversedBy": "subsection"
    },
    "users_who_completed": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "subsections_completed"
    },
    "content": {
      "type": "richtext"
    },
    "files": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "section": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::section.section",
      "inversedBy": "subsections"
    }
  }
}
