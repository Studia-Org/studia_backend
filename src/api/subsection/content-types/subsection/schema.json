{
  "kind": "collectionType",
  "collectionName": "subsections",
  "info": {
    "singularName": "subsection",
    "pluralName": "subsections",
    "displayName": "Subsection",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "fase": {
      "type": "string",
      "required": true
    },
    "finished": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "start_date": {
      "type": "datetime"
    },
    "end_date": {
      "type": "datetime"
    },
    "activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity.activity"
    },
    "paragraphs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::paragraph.paragraph"
    },
    "description": {
      "type": "string",
      "maxLength": 150,
      "minLength": 1,
      "required": true
    },
    "landscape_photo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
